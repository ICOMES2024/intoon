/*인덱스 페이지*/
.container.index {padding:0; height:100%; position:relative; overflow:hidden;}
.container.index img {position:absolute; top:50%; left:50%; transform:translate(-50%,-50%); width:100%; /*min-height:100%;*/}
.index_video {width: 21%; height: 25%; position: absolute;}
.index_video > div {width:100%; height:100%; clip-path:polygon(2.5% 4%, 97% 19%, 96% 92%, 0% 87%);}
.index_video video {position: absolute; top: 0; left: 0; transform: perspective(300px) rotateY(5.5deg) rotateZ(4deg); width:112%; height:100%;}
.index_location > li {border:2px solid transparent; display:block; width:150px; height:150px; position:absolute;}
.index_location li.lecture {width:18%;}
.index_location li.eposter {width:8%;}
.index_location li.online_booth {width:12.5%;}
.index_location li.event_zone {width:8.2%;}
.index_location li.registration {width:20.4%;}
.index_location li.program, .index_location li.mypage, .index_location li.abstract {width:6%;}
.index_location li.partner_slick {width:31.5%; background:#FFF;}
.index_location li a {display:block; width:100%; height:100%;}
.container.index .index_location li a > img {position:absolute; top:4%; transition:all 0.4s;}
.container.index .index_location li a > img.hover_img {opacity:0;}
.container.index .index_location li a:hover > img {opacity:0;}
.container.index .index_location li a:hover > img.hover_img {opacity:1;}
.container.index .index_location li.booth1 a > img {top: -5%;}
.container.index .index_location li.booth2 a > img {top: -2%;}
.container.index .index_location li.booth3 a > img {top: 1%;}
.container.index .index_location li.pos1 a > img {top:21%; width: calc(100% + 10px);}
.container.index .index_location li.pos2 a > img {top:23%; width: calc(100% + 22px); margin-left:6%;}
.container.index .index_location li.pos3 a > img {top:23.5%; width: calc(100% + 22px); margin-left:6%;}
.index .slick-slider {padding:0; height:100%;}
.index .slick-slider .slick-slide {height:100%;}
.index .slick-slider .slick-arrow {display:none !important;}
.index .slick-list, .index .slick-track {padding:0; height:100%;}
.index .slick-slider img {position:static; transform:none; height:50px;}

/*동영상 팝업*/
