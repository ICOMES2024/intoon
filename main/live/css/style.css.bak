/*테이블 팝업*/
.table_pop .pop_contents {padding:0; background-color:#fff;}
.table_pop .pop_title {line-height: 15px; padding:25px 30px 20px; background-color:#ADF002; border-radius:11px 11px 0 0; margin-bottom:0;}
