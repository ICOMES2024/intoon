.table_pop .tab_pager li.on {border-color:#B3F115;}
.table_pop .tab_pager li.on a {color:#000;}
.custom_table {margin:30px 0 20px; background-color:#D9D9D9; border-radius:16px;}
.custom_table th, .custom_table td {border:none; position:relative;}
.custom_table th:after, .custom_table td:after {content:''; display:block; width:1px; height:60%; background:#808082; position:absolute; top:50%; right:0; transform:translateY(-50%);}
.custom_table td:last-child:after {display:none;}
.custom_table th {font-size:14px;}
.custom_table td {font-size:12px;}
.table_bt th, .table_bt td {border-bottom:none; border-top:1px solid #808082;}
.table_pop th:last-child, .table_pop td:last-child {border-right:none;}
.table_pop {font-size:14px;}
.table_pop .tab_cont {max-height:450px; overflow-y:auto;}
