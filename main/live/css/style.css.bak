.lecture_info li span.light {font-weight:300;}
.lecture_info li.under_line > div:first-child span:after {content:''; display:block; width:100%; height:3px; background-color:#C9FFFB; position:absolute; bottom:-4px; left:0;}
.panel, .chairperson {font-weight:500;}
.popup .speaker {float:left; width:calc(100% - 110px); padding-left:20px; height: 320px; overflow-y: auto;}
.popup .speaker li {padding:18px 0; border-top:1px dashed #707070;}
.popup .speaker li:first-child {border:none; padding-top:2px;}
.popup .speaker p {font-weight:300; font-family:"lato", sans-serif; margin-top:10px; color:#000; margin-top:0;}
