.lecture_info li.under_line {margin-bottom:20px;}
.lecture_info li:after {content:''; display:block; clear:both;}
.lecture_info li div {float:left; width:calc(100% - 110px); padding-left:20px; font-family:"lato", sans-serif; line-height:20px;}
.lecture_info li div:first-child {width:110px; text-align:right; padding-left:0;}
